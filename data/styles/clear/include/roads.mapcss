line|z16[hwtag=oneway]::arrows
{
  pattern-offset: 160;
  z-index: 1000;
  pattern-image: arrow-s.svg;
  pattern-spacing: 160;
  z-index: 500;
}

line|z17[hwtag=oneway]::arrows
{
