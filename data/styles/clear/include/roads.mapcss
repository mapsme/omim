line|z18-[highway=pedestrian][bridge?]::bridgeblack,
line|z18-[highway=footway][bridge?]::bridgeblack,
{
  z-index:390;
  casing-width: eval(prop("width")+0.6);
  casing-linecap: butt;
  casing-color: @bridge_casing;
}



